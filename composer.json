{
    "name": "dclaysmith/LaravelAWSElasticsearch",
    "description": "Elastic Driver for Laravel Scout",
    "keywords": ["laravel", "scout", "elasticsearch", "elastic"],
    "require": {
        "php": ">=5.6.4",
        "laravel/scout": "^3.0",
        "jsq/amazon-es-php": "^0.2.0",
        "elasticsearch/elasticsearch": "^5.0"
    },
     "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "dclaysmith\\LaravelAWSElasticsearch\\": "src/"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^5.0)."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
